function enableCrossBorderQB( iSTA )
{
	var xFound = false;
	if( gCBLocationCount >0 && !gCrossBorderActive && gLocCBAllowed && !gCrossBorderEnabled  ){
		for( var i=0;i< gCBCountry2List.length;i++) {
