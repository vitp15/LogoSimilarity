			}
		}
		if( xFound  ) {
			$('#ettcountry_ro').show().find('a').trigger('click');
			$('.select_replace:has(#ettcountry)').show(); // alt. if ettcountry has been activated
			gLocCrossBorderEnabled =1;
		} else {
